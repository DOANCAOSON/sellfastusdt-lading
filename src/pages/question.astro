    </div>
  </section>
</Layout>

<script>
  document.querySelectorAll('.btn-question').forEach(function (icon, index) {
    icon.addEventListener('click', function () {
      var dropdown = document.querySelectorAll('.question-dropdows')[index] as any
      if (dropdown.style.height === '0px' || dropdown.style.height === '') {
        dropdown.style.height = 'auto'
      } else {
        dropdown.style.height = '0px'
      }
    })
  })
</script>
